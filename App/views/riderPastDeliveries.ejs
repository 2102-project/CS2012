<head>
        <!-- Page Title -->
        <title>CS2102 Starter Guide</title>
        <link rel='stylesheet' href='/stylesheets/style.css' />

        <!-- CSS for Styling -->
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css"
                integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS"
                crossorigin="anonymous">

        <!-- JavaScript for Interactivity -->
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
                integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
                crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js"
                integrity="sha384-wHAiFfRlMFy6i5SRaxvfOCifBUQy1xHdJ/yoi7FRNXMRBu5WHdZYu1hA6ZOblgut"
                crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"
                integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k"
                crossorigin="anonymous"></script>
</head>

<body>
        <h1><%= sess.name%></h1>
        <p>Here are your past deliveries</p><br />
        <p>Keep up the good work!</p>
        <table class="table">
                <thead>
                        <tr>
                                <th scope="col">Delivery id</th>
                                <th scope="col">Delivery fee</th>
                                <th scope="col">Order placed on</th>
                                <th scope="col">Order accepted time</th>
                                <th scope="col">Arrival time at restaurant</th>
                                <th scope="col">Departure time from restaurant</th>
                                <th scope="col">Delivery completed at</th>
                                <th scope="col">Rating from customer</th>
                        </tr>
                </thead>
                <tbody>
                        <% for(var i=0; i<deliverydata.length; i++) { %>
                        <tr>
                                <td><%= deliverydata[i].did%></td>
                                <td><%= deliverydata[i].deliveryfee%></td>
                                <td><%= deliverydata[i].customerplaceorder%></td>
                                <td><%= deliverydata[i].ridergotorest%></td>
                                <td><%= deliverydata[i].rideratrest%></td>
                                <td><%= deliverydata[i].riderleftrest%></td>
                                <td><%= deliverydata[i].riderdeliverorder%></td>
                                <td><%= deliverydata[i].rating%></td>
                        </tr>
                        <% } %>
                </tbody>
        </table>

        <p><a href="/riderProfile">Click here to go back</a></p>

</body>

</html>